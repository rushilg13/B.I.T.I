<script>

    function getRandomInt(min, max) {
        min = Math.ceil(min);
        max = Math.floor(max);
        return Math.floor(Math.random() * (max - min) + min);
    }

    var _img = document.getElementById('profile_image');
    var newImg = new Image;
    newImg.onload = function () {
        _img.src = this.src;
    }
    let num = getRandomInt(1, 50);
    newImg.src = `https://xsgames.co/randomusers/assets/avatars/pixel/${num}.jpg`;
</script>

