<script src="https://cdn.jsdelivr.net/npm/chart.js@4.0.1/dist/chart.umd.min.js"></script>
<!-- Page level custom scripts -->
<script>

    function toMonthName(monthNumber) {
        const date = new Date();
        date.setMonth(monthNumber - 1);

        return date.toLocaleString('en-US', {
            month: 'long',
        });
    }
    const ctx = document.getElementById('myChart');
    const d = new Date();
    const curr_month = d.getMonth() + 1;
    let ytd_months = []
    for (let year = 1; year <= curr_month; year++) {
        ytd_months.push(toMonthName(year));
    }
    new Chart(ctx, {
        type: 'bar',
        data: {
            labels: ytd_months,
            datasets: [{
                label: 'Earnings YTD',
                data: [<%= Object.values(bar_chart_stats) %>],
                backgroundColor: [
                    'rgba(255, 99, 132, 0.2)',
                    'rgba(255, 159, 64, 0.2)',
                    'rgba(255, 255, 86, 0.2)',
                    'rgba(75, 192, 192, 0.2)',
                    'rgba(54, 162, 235, 0.2)',
                    'rgba(153, 102, 255, 0.2)',
                    'rgba(201, 203, 207, 0.2)',
                    'rgba(255, 0, 157, 0.2)',
                    'rgba(76, 0, 153, 0.2)'
                ],
                borderColor: [
                    'rgb(255, 99, 132)',
                    'rgb(255, 159, 64)',
                    'rgb(255, 205, 86)',
                    'rgb(75, 192, 192)',
                    'rgb(54, 162, 235)',
                    'rgb(153, 102, 255)',
                    'rgb(201, 203, 207)',
                    'rgb(255, 0, 157)',
                    'rgb(76, 0, 153)'


                ],
                borderWidth: 1
            }]
        },
        options: {
            plugins: {
                legend: {
                    display: false
                }
            },
            scales: {
                y: {
                    beginAtZero: true
                }
            }
        }
    });

    const pie_ctx = document.getElementById('myPieChart');
    new Chart(pie_ctx, {
        type: 'pie',
        data: {
            labels: ["On/Before Time Delivery", "Delayed Delivery"],
            datasets: [{
                label: 'Delivery Stats',
                data: [<%=Object.values(delivery_stats) %>],
                backgroundColor: [
                    'rgba(101, 255, 64, 0.8)',
                    'rgba(255, 100, 64, 0.8)'
                ]
            }]
        }
    });

    const payment_method_chart = document.getElementById('paymentMethod_chart');
    new Chart(payment_method_chart, {
        type: 'bar',
        data: {
            labels: ["Credit Card",
                "Debit Card",
                "Cash",
                "UPI",
                "Bank Transfer",
                "Online Wallets",
                "Other"],
            datasets: [{
                data: [<%= Object.values(paymentMethod_stats) %>],
                backgroundColor: [
                    'rgba(255, 99, 132, 0.2)',
                    'rgba(255, 159, 64, 0.2)',
                    'rgba(255, 255, 86, 0.2)',
                    'rgba(75, 192, 192, 0.2)',
                    'rgba(54, 162, 235, 0.2)',
                    'rgba(153, 102, 255, 0.2)',
                    'rgba(201, 203, 207, 0.2)',
                ],
                borderColor: [
                    'rgb(255, 99, 132)',
                    'rgb(255, 159, 64)',
                    'rgb(255, 205, 86)',
                    'rgb(75, 192, 192)',
                    'rgb(54, 162, 235)',
                    'rgb(153, 102, 255)',
                    'rgb(201, 203, 207)',
                ],
                borderWidth: 1
            }]
        },
        options: {
            plugins: {
                legend: {
                    display: false
                }
            },
            scales: {
                y: {
                    beginAtZero: true
                }
            }
        }
    });

    var cats = ["Books",
        "Car & Automobiles",
        "Clothing & Accessories",
        "Electronics",
        "Grocery & Gourmet",
        "Health & Personal Care",
        "Home & Kitchen",
        "Jewellery",
        "Musical Instruments",
        "Office Product",
        "Shoes & Handbag",
        "Sports & Fitness",
        "Toys & Games",
        "Watches"];
    var vals = [<%=Object.values(order_categories) %>];
    var n = cats.length;
    let cat_vals = {}
    for (let i = 0; i < n; i++) {
        if (vals[i] != 0) {
            cat_vals[cats[i]] = vals[i];
        }
    }
    const orderCategories_pie_chart = document.getElementById('orderCategories_pie');
    new Chart(orderCategories_pie_chart, {
        type: 'pie',
        data: {
            labels: Object.keys(cat_vals),
            datasets: [{
                label: 'Order Categories Stats',
                data: Object.values(cat_vals),
                backgroundColor: [
                    'rgba(255, 99, 132, 0.8)',
                    'rgba(255, 159, 64, 0.8)',
                    'rgba(255, 255, 86, 0.8)',
                    'rgba(75, 192, 192, 0.8)',
                    'rgba(54, 162, 235, 0.8)',
                    'rgba(153, 102, 255, 0.8)',
                    'rgba(201, 203, 207, 0.8)',
                ]
            }]
        }
    });
</script>

